		{
			"name": "Post",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response exists or not\", function()",
							" {",
							"    pm.expect(pm.response.text()).to.not.be.empty;",
							"});",
							"",
							"pm.test(\"Verify property exists\", function()",
							" {",
							"    pm.expect(pm.response.json()).to.have.property(\"ProductName\");",
							"});",
							"",
							"pm.test(\"Verify value of the property exists\", function()",
							" {",
							"    pm.expect(Number(pm.response.json().Price)).to.be.above(0);",
							"});",
							"",
							"pm.test(\"Verify value datatype\", function()",
							" {",
							"    pm.expect(pm.response.json().Price).to.be.a(\"string\");",
							"});",
							"",
							"pm.test(\"Product price value below range check\", function()",
							" {",
							"    pm.expect(Number(pm.response.json().Price)).to.be.below(5000);",
							"});",
							"",
							"pm.test(\"Verify response time\", function() ",
							"{",
							"    pm.expect(pm.response.responseTime).to.be.below(10000);",
							"});",
							"",
							"pm.test(\"Verify response time above\", function()",
							" {",
							"    pm.expect(pm.response.responseTime).to.be.above(0);",
							"});",
							"",
							"pm.test(\"Check multiple headers\", function()",
							" {",
							"    const headers = [\"content-length\", \"content-type\", \"date\"];",
							"    headers.forEach(h => {",
							"        pm.response.to.have.header(h);",
							"    });",
							"});",
							"",
							"pm.test(\"Test status code using if-else\", function()",
							" {",
							"    const data=pm.response.code;",
							"    if (data === 200) {",
							"        pm.response.to.have.status(\"OK\");",
							"    } else",
							"    {",
							"        pm.expect(data).to.not.eql(200);",
							"    }",
							"});",
							"",
							"pm.test(\"Status code is not in oneOf function\", function()",
							" {",
							"    pm.expect(pm.response.code).to.not.oneOf([400,401,402,403,404]);",
							"});",
							"",
							"",
							"",
							"pm.collectionVariables.set(\"id\",pm.response.json().id);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
